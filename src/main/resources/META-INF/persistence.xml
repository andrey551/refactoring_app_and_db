<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="tad">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <jta-data-source>java:/PostgresDS</jta-data-source>
        <class>Model.Account</class>
        <class>Model.User</class>
        <class>Model.Record</class>
        <class>Model.Location</class>
        <class>Model.Specialization</class>
        <class>Model.HospSpec</class>
        <class>Model.Wallet</class>
        <class>Model.Comment</class>
        <class>Model.Chat</class>
        <class>Model.Consultant</class>
        <class>Raw.RawLocation</class>
        <class>Model.Doctor</class>
        <class>Model.Education</class>
        <class>Model.Education</class>
        <class>Model.Info</class>
        <class>Model.Message</class>
        <class>Model.Prize</class>
        <class>Model.CurrencyUnit</class>
        <class>Raw.RawComment</class>
        <class>Raw.coordinate</class>
        <class>Raw.WrapComment</class>
        <properties>
            <property name="hibernate.show_sql" value="true" />
            <property name="hibernate.format_sql" value="true" />
            <property name="hibernate.hbm2ddl.auto" value="update" />
            <property name="hibernate.generate_statistics" value="true"/>
        </properties>
    </persistence-unit>

</persistence>